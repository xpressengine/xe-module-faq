<query id="getAllFaq" action="select">
	<tables>
		<table name="modules" />
	</tables>
	<columns>
		<column name="*" />
	</columns>
	<conditions>
		<condition operation="equal" column="module" default="faq" />
	</conditions>
	<navigation>
		<index var="sort_index" default="mid" order="asc" />
	</navigation>
</query>
